    "solid-js"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.5",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-solid": "^2.10.2"
  },
  "peerDependencies": {
    "solid-js": "^1.8.16"
  }
}