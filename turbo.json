{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "ui": {
      "dependsOn": [
        "^ui"
      ]
    },
    "dev": {
      "dependsOn": [
        "ui"
      ],
      "cache": false
    }
  }
}